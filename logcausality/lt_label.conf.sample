[group_mgmt]
members = user, remote, syslog

[group_l7]
members = dns, dhcp, snmp, ntp, syslog


[label_boot]
rules = shutdown, reboot
shutdown_word = shutdown
shutdown_rule = in_ord
reboot_word = reboot
reboot_rule = in_ord

[label_user]
rules = su, sudo
su_word = su
su_rule = equal
sudo_word = sudo
sudo_rule = equal

[label_remote]
rules = sshd
sshd_word = sshd
sshd_rule = equal

[label_dns]
rules = named, dns
named_word = named
named_rule = equal
dns_word = DNS
dns_rule = in_ord

[label_dhcp]
rules = dhcpd
dhcpd_word = dhcpd
dhcpd_rule = equal

[label_snmp]
rules = snmpd
snmpd_word = snmpd
snmpd_rule = equal

[label_ntp]
rules = ntpd
ntpd_word = ntpd
ntpd_rule = equal

[label_syslog]
rules = syslogd, newsyslog, rotate
syslogd_word = syslogd
syslogd_rule = equal
newsyslog_word = newsyslog
newsyslog_rule = equal
rotate_word = last, message, repeated
rotate_rule = equal, equal, equal

